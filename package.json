{
  "name": "@michijs/michijs-template",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/michijs/michijs-template.git"
  },
  "description": "Template for MichiJS library",
  "scripts": {
    "start": "michi-server --start",
    "dist": "michi-server --dist",
    "build": "michi-server --build",
    "publish-npm": "npm run dist && npm publish --access public",
    "generate-icons": "michi-server --generate-assets",
    "test-tsc": "michi-server --test-tsc"
  },
  "keywords": [
    "web-components",
    "michijs"
  ],
  "devDependencies": {
    "@michijs/tsconfig": "0.0.4",
    "@michijs/dev-server": "0.8.5",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@michijs/default-service-worker": "0.0.5",
    "@michijs/michijs": "2.3.9"
  }
}
