{
  "name": "@michijs/michijs-template",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/michijs/michijs-template.git"
  },
  "description": "Template for MichiJS library",
  "scripts": {
    "start": "michi-server --start",
    "dist": "michi-server --dist",
    "build": "michi-server --build",
    "publish-npm": "npm run dist && npm publish --access public",
    "generate-icons": "michi-server --generate-icons",
    "test-tsc": "tsc --noEmit --skipLibCheck",
    "sandbox": "npm install && npm run start"
  },
  "keywords": [
    "web-components",
    "michijs"
  ],
  "devDependencies": {
    "@michijs/dev-server": "0.5.9",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "@michijs/michijs": "1.1.7"
  }
}
